import csv
from fillpdf import fillpdfs

n = 0

with open('fillsf180template - Template.csv', newline='') as csvfile:
    # edit to name of csv file
    reader = csv.DictReader(csvfile)
    for row in reader:
        n += 1

        data_dict = {'VetName': row['VetName'],
        'SSN': row['SSN'],
        'BirthDate': row['BirthDate'],
        'BirthPlace': row['BirthPlace'],
