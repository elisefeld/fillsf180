        'IsUnretired': row['IsUnretired'],
        'IsRetired': row['IsRetired'],
        'IsDD214': row['IsDD214'],
        'SeparationDate': row['SeparationDate'],
        'IsDeleted': row['IsDeleted'],
        'IsOMPF': row['IsOMPF'],
